{
  "name": "soberwp/themer",
  "type": "wordpress-muplugin",
  "license": "MIT",
  "description": "WordPress plugin for themeing using JSON.",
  "homepage": "https://github.com/soberwp",
  "authors": [
    {
      "name": "Darren Jacoby",
      "email": "darren@jacoby.co.za",
      "homepage": "https://github.com/darrenjacoby"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "support": {
    "issues": "https://github.com/soberwp/themer/issues"
  },
  "require": {
    "php": ">=5.4.0",
    "composer/installers": "~1.0",
    "hassankhan/config": "^0.10.0",
    "tgmpa/tgm-plugin-activation": "^2.6"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "2.*"
  },
  "autoload": {
    "psr-4": {
      "Sober\\Themer\\": "src/",
      "Sober\\Themer\\Module\\": "src/Module/"
    }
  },
  "scripts": {
    "test": [
      "vendor/bin/phpcs --extensions=php --ignore=vendor/ ."
    ]
  }
}
